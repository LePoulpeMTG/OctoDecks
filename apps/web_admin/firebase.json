{
  "hosting": [
    {
      "target": "public",
      "site": "octodecks",
      "public": "apps/web_user/build/web",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
    },
    {
      "target": "admin",
      "site": "adminoctodecks",
      "public": "build/web",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "/sets.json",
          "destination": "/__/storage/o/exports%2Fsets.json?alt=media"
        }
      ]
    }
  ]
}
