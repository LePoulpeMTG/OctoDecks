{
  "hosting": [
    {
      "target": "public",
      "public": "build/web",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
    },
    {
      "target": "admin",
      "public": "public",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],

      "rewrites": [
        {
          "source": "/sets.json",
          "destination": "/__/storage/o/exports%2Fsets.json?alt=media"
        }
      ]
    }
  ]
}
